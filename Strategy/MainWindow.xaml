<Window x:Class="Strategy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Strategy"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,10,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="MainMenu" Content="Frame" HorizontalAlignment="Center" Height="424" VerticalAlignment="Top" Width="800" Navigated="Frame_Navigated" Margin="0,-10,0,0">
            <Frame.Background>
                <ImageBrush ImageSource="/maxresdefault.jpg"/>
            </Frame.Background>
        </Frame>
        <Button x:Name="EntryBut" Content="Enter" HorizontalAlignment="Left" Height="98" Margin="215,0,0,0" VerticalAlignment="Center" Width="395" Click="Enter_Click"/>
        <!--<Button x:Name="Entry" Content="Button" HorizontalAlignment="Left" Height="57" Margin="655,330,0,0" VerticalAlignment="Top" Width="101" Click="Button_Click"/>-->

    </Grid>
</Window>
